  },
  "include": [
    "./**/*.ts",
    "./types/**/*.d.ts"
  ],
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.3",
    "@fastify/jwt": "^10.0.0",
    "@fastify/type-provider-typebox": "^6.1.0",
    "bcrypt": "^6.0.0",
    "fastify": "^5.6.1",
    "typebox": "^1.0.50"
  },
  "devDependencies": {
    "@prisma/client": "^6.18.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.10.0",
    "prisma": "^6.18.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "compilerOptions": {
